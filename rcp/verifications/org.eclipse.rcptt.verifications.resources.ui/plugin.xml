<plugin>
   <extension
         point="org.eclipse.rcptt.ui.verificationViewers">
      <viewer
            class="org.eclipse.rcptt.verifications.resources.ui.viewers.WorkspaceVerificationViewer"
            icon="/icons/workspace.gif"
            type="org.eclipse.rcptt.verifications.workspace">
      </viewer>
   </extension>
   <extension
         point="org.eclipse.rcptt.ui.searchMatchers">
      <matcher
            class="org.eclipse.rcptt.verifications.resources.ui.WorkspaceVerificationMatcher">
      </matcher>
   </extension>
   <extension
         point="org.eclipse.rcptt.core.builder.q7Validator">
      <validator
            class="org.eclipse.rcptt.verifications.resources.ui.WorkspaceVerificationContentValidator">
      </validator>
   </extension>
</plugin>
