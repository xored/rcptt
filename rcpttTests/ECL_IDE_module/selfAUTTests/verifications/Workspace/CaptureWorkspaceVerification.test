get-view "Test Explorer" | get-tree | select "MyProject/WorkspaceContext" | double-click
get-editor WorkspaceContext | get-button Apply | click
get-view "Test Explorer" | get-tree | select MyProject | get-menu -path "New/Verification" | click
with [get-window "New Verification Wizard"] {
    get-editbox -after [get-label "Name:"] | set-text Verification
    get-button Finish | click
}
get-editor Verification | get-button Capture | click
get-button "Save (M1+S)" | click
get-editor Verification | get-button Verify | click
get-view "Execution View" | get-tree | get-item -path "Verification.*" | get-property "image.path" 
    | equals "org.eclipse.rcptt.ui/icons/launching/verification-pass.png" | verify-true
